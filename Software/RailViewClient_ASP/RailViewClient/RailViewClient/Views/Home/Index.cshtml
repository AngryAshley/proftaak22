@model List<Alert>
@{
    ViewData["Title"] = "Home Page";
}

<div class="container">
    <div class="row">
        <div class="col col-lg-3">
            <h4>Meldingen</h4>
            <div id="log" class="border border-dark border-2">
                @foreach (var item in Model)
                {
                    <div class="text-center border-top border-dark border-2">
                        <p class="px-2 pt-2 text-capitalize">
                            @item.times<br />@item.route<br /><b>@item.alert</b>
                        </p>
                    </div>
                }
            </div>

            @*<a class="navbar-brand" asp-controller="Home" asp-action="Privacy">*@
            <input type="button" class="btn btn-success my-2" id="btnpopup" value="Bel 112" onclick="ShowPopUp();"/>@*Bel 112</input>*@
            @*</a>*@

        </div>
        <div class="col col-lg-3" id="test">
            <div id="map"></div>
        </div>
    </div>
</div>

